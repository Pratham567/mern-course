<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Categories</title>
</head>
<body>
    <% categories.forEach(category => { %>
        <h2><%= category.name %></h2>
        <ul>
            <% category.items.forEach(item => { %>
                <li><%= item %></li>
            <% }); %>
        </ul>
    <% }); %>
</body>
</html>